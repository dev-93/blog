{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "globals": {
        "Atomics": "readonly",
        "ShareArrayBuffer": "readonly"
    },
    "plugins": ["react"],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off"
    }
}
